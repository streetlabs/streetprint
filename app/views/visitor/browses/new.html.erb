<% title "Browse", false %>

<% liquid_args = add_common_liquid_args %>
<% liquid_args['browse_by_list'] = render(:file => 'visitor/browses/_browse_by.html.erb') %>
<% liquid_args['options_list'] = render(:file => 'visitor/browses/_options.html.erb')  if @options%>
<% liquid_args['options'] = @options %>

<% template = Liquid::Template.parse(@site.site_theme.browse_artifacts_template.template) %>
<%= template.render(liquid_args, :filters => [LiquidFilters]) %>


<% content_for :css do %>
  <% template = Liquid::Template.parse( @site.site_theme.browse_artifacts_template.css) %>
  <%= template.render(liquid_args, :filters => [LiquidFilters]) %>
<% end %>