<% title "Google location page" %>

<% form_for [@show_google_location_template], :builder => AdminFormBuilder, :url => { :controller => :show_google_location_template, :action => :update } do |f| %>
  <fieldset>
    <legend>Google Location Template</legend>
    <%= f.error_messages %>
    <%= f.text_area :template, :rows=>20 %>
    <%= f.text_area :css, :rows=>20 %>
    <div class="submit"><%= f.submit "Save" %></div>
  </fieldset>
<% end %>


